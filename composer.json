{
	"description": "A bot that posts pictures from teh German Weather Service webcams.",
	"homepage": "https://github.com/php-tootbot/dwd-webcam",
	"license": "MIT",
	"type": "project",
	"authors": [
		{
			"name": "Contributors",
			"homepage":"https://github.com/php-tootbot/dwd-webcam/graphs/contributors"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php": "^8.1",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-openssl": "*",
		"chillerlan/php-settings-container": "^2.1.4 || ^3.1",
		"php-tootbot/php-tootbot": "dev-main#7b1f2573d36cb5984ea46e9dc5aa0575e83bd28b"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.2",
		"phan/phan": "^5.4"
	},
	"autoload": {
		"psr-4": {
			"PHPTootBot\\DWDWebcamBot\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PHPTootBot\\DWDWebcamBotTest\\": "tests/"
		}
	},
	"scripts": {
		"phpunit": "@php vendor/bin/phpunit",
		"phan": "@php vendor/bin/phan"
	},
	"config": {
		"lock": true,
		"sort-packages": true,
		"platform-check": true
	}
}
